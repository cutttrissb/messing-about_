<h1>Listing product_archives</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Price</th>
    <th>Currency</th>
    <th>External</th>
    <th>Created at</th>
    <th>Provider</th>
  </tr>

<% for product_archive in @product_archives %>
  <tr>
    <td><%=h product_archive.title %></td>
    <td><%=h product_archive.price %></td>
    <td><%=h product_archive.currency %></td>
    <td><%=h product_archive.external_id %></td>
    <td><%=h product_archive.created_at %></td>
    <td><%=h product_archive.provider %></td>
    <td><%= link_to 'Show', product_archive %></td>
    <td><%= link_to 'Edit', edit_product_archive_path(product_archive) %></td>
    <td><%= link_to 'Destroy', product_archive, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_archive', new_product_archive_path %>
